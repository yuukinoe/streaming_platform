{
	"name": "streaming_client",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@flowbite-svelte-plugins/chart": "^0.2.4",
		"@flowbite-svelte-plugins/texteditor": "^0.25.2",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.8.1",
		"flowbite-svelte-admin-dashboard": "^1.0.6",
		"flowbite-svelte-icons": "^2.2.1",
		"globals": "^16.0.0",
		"lowlight": "^3.3.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vite-plugin-devtools-json": "^0.2.0"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@flowbite-svelte-plugins/datatable": "^0.4.0",
		"@lucide/svelte": "^0.523.0",
		"@splidejs/splide": "^4.1.4",
		"@splidejs/svelte-splide": "^0.2.9",
		"@tiptap/core": "3.0.7",
		"@tiptap/pm": "^3.0.9",
		"@tiptap/starter-kit": "^3.0.9",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.2.0",
		"bits-ui": "^2.8.11",
		"js-yaml": "^4.1.0",
		"phosphor-svelte": "^3.0.1"
	}
}
